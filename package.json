{
  "$schema": "https://json.schemastore.org/package",
  "name": "catalyst-ui",
  "author": "TheBranchDriftCatalyst",
  "private": false,
  "version": "0.0.0",
  "type": "module",
  "main": "./dist/catalyst.js",
  "types": "./dist/catalyst.d.ts",
  "files": [
    "dist",
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/TheBranchDriftCatalyst/catalyst-ui"
  },
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "dev": "foreman start -f Procfile.dev",
    "dev:app": "vite",
    "dev:preview": "vite preview ./app",
    "dev:storybook": "storybook dev -p 6006",
    "dev:lib": "vite build --watch",
    "dev:test": "test-storybook --coverage",
    "build": "tsc --p ./tsconfig-build.json && vite build",
    "build:app": "vite build ./app",
    "build:storybook": "storybook build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "release:first": "standard-version --first-release",
    "release:patch": "standard-version --release-as patch",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "prepare": "husky"
  },
  "postinstall": "yarn playwright install",
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slider": "^1.1.2",
    "@storybook/addon-docs": "^9.1.10",
    "@storybook/addon-jest": "^9.1.10",
    "@storybook/addon-links": "^9.1.10",
    "@storybook/addon-onboarding": "^9.1.10",
    "@storybook/addon-themes": "^9.1.10",
    "@storybook/react-vite": "^9.1.10",
    "@storybook/test-runner": "^0.23.0",
    "@tailwindcss/vite": "^4.1.14",
    "@types/d3": "^7.4.3",
    "@types/js-yaml": "^4.0.9",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "debug": "^4.3.4",
    "eslint": "^9.3.0",
    "eslint-plugin-react-refresh": "^0.4.7",
    "eslint-plugin-storybook": "^9.1.10",
    "husky": "^9.1.7",
    "jest-image-snapshot": "^6.4.0",
    "lodash": "^4.17.21",
    "prettier": "^3.2.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-test-renderer": "^18.3.1",
    "standard-version": "^9.5.0",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "vite-plugin-dts": "^3.9.1",
    "vite-plugin-dynamic-import": "^1.5.0",
    "vite-plugin-istanbul": "^6.0.2",
    "vite-plugin-lib-inject-css": "^2.1.1",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "dependencies": {
    "@faker-js/faker": "^10.0.0",
    "@hookform/resolvers": "^5.2.2",
    "@icons-pack/react-simple-icons": "^13.8.0",
    "@jdion/tilt-react": "^1.0.0",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-navigation-menu": "^1.1.4",
